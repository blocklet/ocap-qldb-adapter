{
  "name": "ocap-qldb-adapter",
  "title": "QLDB Manager",
  "version": "0.5.0",
  "description": "Simple app that serve an OCAP adapter which uses AWS QLDB as backend",
  "keywords": [
    "qldb",
    "adapter"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .blocklet",
    "start": "DEBUG=@ocap/*,@arcblock/arcblock* node index.js",
    "prepublishOnly": "npm run build",
    "build": "DEBUG=@abtnode/* NODE_ENV=production an bundle -w webpack.blocklet.js --create-release",
    "build-debug": "DEBUG=@abtnode/* NODE_ENV=development abtnode bundle -w webpack.blocklet.js",
    "release": "npm run clean && npm run build",
    "deploy": "npm run release && an deploy .blocklet/bundle --skip-hooks"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "wangshijun <wangshijun2010@gmail.com> (http://github.com/wangshijun)",
  "license": "MIT",
  "dependencies": {
    "@ocap/adapter-qldb": "^0.4.5",
    "@ocap/driver-qldb": "^3.2.0",
    "@ocap/resolver": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  }
}
